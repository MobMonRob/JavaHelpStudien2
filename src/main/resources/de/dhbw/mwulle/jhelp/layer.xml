<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="com-mwulle-jHelp-ui-HelpTopComponent.instance">
                <attr name="iconBase" stringvalue="com/mwulle/jHelp/ui/jHelp-16x16.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
                <attr name="preferredID" stringvalue="HelpViewerTopComponent"/>
            </file>
            <file name="com-mwulle-jHelp-ui-HelpViewerTopComponent.instance_hidden"/>
        </folder>
    </folder>

    <folder name="Services">
        <folder name="JavaHelp">
            <!-- Need to keep this here,
            since for whatever reason xml/entities is not used
            when reading the xml entity from the Services folder.
            TODO 2024-03-18: Look into why it is not working -->
            <file name="helpsetlink.dtd" url="dtd/helpsetlink.dtd"/>
        </folder>
    </folder>

    <folder name="xml">
        <folder name="entities">
            <folder name="DHBW_Karlsruhe">
                <file name="DTD_JavaHelp_Help_Set_link_1_0" url="helpsetlink.dtd">
                    <attr name="hint.originalPublicID"
                          stringvalue="-//DHBW_Karlsruhe//DTD JavaHelp Help Set link 1.0//EN"/>
                </file>
            </folder>
            <folder name="Sun_Microsystems_Inc_">
                <file name="DTD_JavaHelp_HelpSet_Version_2_0" url="dtd/helpset.dtd">
                    <attr name="hint.originalPublicID"
                          stringvalue="-//Sun Microsystems Inc.//DTD JavaHelp HelpSet Version 2.0//EN"/>
                </file>
                <file name="DTD_JavaHelp_Index_Version_2_0" url="dtd/index.dtd">
                    <attr name="hint.originalPublicID"
                          stringvalue="-//Sun Microsystems Inc.//DTD JavaHelp Index Version 2.0//EN"/>
                </file>
                <file name="DTD_JavaHelp_Map_Version_2_0" url="dtd/map.dtd">
                    <attr name="hint.originalPublicID"
                          stringvalue="-//Sun Microsystems Inc.//DTD JavaHelp Map Version 2.0//EN"/>
                </file>
                <file name="DTD_JavaHelp_TOC_Version_2_0" url="dtd/toc.dtd">
                    <attr name="hint.originalPublicID"
                          stringvalue="-//Sun Microsystems Inc.//DTD JavaHelp TOC Version 2.0//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="DHBW_Karlsruhe">
                <file name="DTD_JavaHelp_Help_Set_link_1_0.instance">
                    <attr name="instanceOf" stringvalue="org.openide.loaders.Environment$Provider"/>
                    <attr name="instanceCreate" newvalue="de.dhbw.mwulle.jhelp.netbeans.impl.HelpSetLinkReader"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
